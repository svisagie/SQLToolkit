{
    "manifestVersion": 1,
    "id": "mixt-sql-toolkit",
    "name": "MiX Telematics SQL Toolkit",
    "version": "1.0.0.10",
    "publisher": "MiXTelematicsCSO",
    "public": false,
	"categories": [
        "Build and release"
    ],
    "links": {
        "support": {
            "uri": "https://github.com/crimdon/SQLToolkit/issues"
        }
    },
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],    
    "description": "SQL Toolkit for on premises TFS",

    "icons": {
        "default": "icon.jpg"        
    },
    "content": {
        "details": {
            "path": "readme.md"
    },
    "license": {
            "path": "LICENSE.txt"
    }
    },
    "files": [
        {
            "path": "RunSingleSqlScript"
        },
        {
            "path": "RunSqlScripts"
        },
        {
            "path": "SqlBackup"
        },
        {
            "path": "RunStoredProcedure"
        },
        {
            "path": "RunSqlCommand"
        },
        {
            "path": "RunDACPAC"
        }
    ],
    "contributions": [
        {
            "id": "runsinglesqlscript",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "RunSingleSqlScript"
            }
        },
        {
            "id": "runsqlscripts",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "RunSqlScripts"
            }
        },
         {
            "id": "sqlbackup",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "SqlBackup"
            }
        },
        {
            "id": "runstoredprocedure",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "RunStoredProcedure"
            }
        },
        {
            "id": "runsqlcommand",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "RunSqlCommand"
            }
        },
        {
            "id": "rundacpac",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "RunDACPAC"
            }
        }
    ]
}